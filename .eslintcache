pageInfo;\r\n        queryVariablesRef.current.before = undefined;\r\n        queryVariablesRef.current.after = endCursor;\r\n        reExcQuery();\r\n    }\r\n\r\n    if(error) return <p>oops error: {error.message}</p>;\r\n    \r\n    if(data || fetching){\r\n            const showPrev = data && data.balloons.pageInfo.hasPreviousPage;\r\n            const showNext = data && data.balloons.pageInfo.hasNextPage;\r\n        return (\r\n            <section id=\"view-cards-section\" >\r\n                <div id=\"ballon-grid\">\r\n                    {renderGridContent(fetching, ballonsList)} \r\n                </div>\r\n                <div className=\"page-buttons\">\r\n                    {(showPrev && !fetching) && (<button onClick={onPrevClick} >◀ prev</button>)}\r\n                    {(showNext && !fetching) && (<button onClick={onNextClick} >next ▶</button>)}\r\n                </div>\r\n            </section>\r\n        );\r\n    }\r\n    return null;\r\n}\r\n\r\nexport default BallonGrid;","C:\\Users\\Josef\\Desktop\\TEST\\react-web-dev\\src\\pages\\HomePage\\BallonGrid\\helperFunctions.tsx",[],"C:\\Users\\Josef\\Desktop\\TEST\\react-web-dev\\src\\pages\\HomePage\\BallonGrid\\renderGridContent.tsx",[],"C:\\Users\\Josef\\Desktop\\TEST\\react-web-dev\\src\\pages\\HomePage\\BallonGrid\\gqlQuery.ts",[],"C:\\Users\\Josef\\Desktop\\TEST\\react-web-dev\\src\\pages\\DetailsPage\\gqlQuery.ts",[],"C:\\Users\\Josef\\Desktop\\TEST\\react-web-dev\\src\\config.ts",[],"C:\\Users\\Josef\\Desktop\\TEST\\react-web-dev\\src\\reducers\\cart.ts",[],"C:\\Users\\Josef\\Desktop\\TEST\\react-web-dev\\src\\pages\\DetailsPage\\DetailsList.tsx",[],"C:\\Users\\Josef\\Desktop\\TEST\\react-web-dev\\src\\pages\\CheckOutPage\\CheckOutPage.tsx",[],{"ruleId":"118","replacedBy":"119"},{"ruleId":"120","replacedBy":"121"},{"ruleId":"122","severity":1,"message":"123","line":1,"column":27,"nodeType":"124","messageId":"125","endLine":1,"endColumn":45},{"ruleId":"126","severity":1,"message":"127","li